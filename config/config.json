{
  "project": {
    "name": "WH40K Tactics RL",
    "version": "1.0.0",
    "description": "Warhammer 40k tactical game with AI opponents"
  },
  "paths": {
    "ai_models": "ai/",
    "models_root": "ai/models",
    "model_file": "ai/models/{agent_key}/model_{agent_key}.zip",
    "scenarios": "config/scenarios.json",
    "rewards": "config/rewards_config.json",
    "training": "config/training_config.json",
    "game_config": "config/game_config.json",
    "macro_controller_config": "config/agents/MacroController/pve_macro_controller.json",
    "tensorboard": "./tensorboard/"
  },
  "defaults": {
    "scenario": "default",
    "training_config": "default",
    "rewards_config": "default",
    "game_config": "default"
  },
  "training_env": {
    "OMP_NUM_THREADS": 1,
    "MKL_NUM_THREADS": 1,
    "OPENBLAS_NUM_THREADS": 1,
    "NUMEXPR_NUM_THREADS": 1,
    "PYTORCH_CUDA_ALLOC_CONF": "expandable_segments:True",
    "CUDA_LAUNCH_BLOCKING": "0"
  },
  "torch": {
    "compile_mode": "off",
    "compile_mode_option_1": "reduce-overhead (default) : CUDA graphs, low overhead",
    "compile_mode_option_2": "max-autotune : CUDA graphs, high overhead",
    "compile_mode_option_3": "default : compilation plus rapide, moins d\u2019optimisations"
  }
}